{
    "name": "rawilk/filament-inner-nav",
    "description": "Add inner navigation to filament panel pages.",
    "keywords": [
        "rawilk",
        "laravel",
        "filament",
        "navigation",
        "filament-plugin"
    ],
    "homepage": "https://github.com/rawilk/filament-inner-nav",
    "license": "MIT",
    "authors": [
        {
            "name": "Randall Wilk",
            "email": "randall@randallwilk.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1|^8.2",
        "illuminate/contracts": "^10.0",
        "spatie/laravel-package-tools": "^1.14"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "spatie/laravel-ray": "^1.31"
    },
    "autoload": {
        "psr-4": {
            "Rawilk\\FilamentInnerNav\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rawilk\\FilamentInnerNav\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@php ./vendor/bin/testbench package:discover --ansi"
        ],
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint --dirty"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rawilk\\FilamentInnerNav\\FilamentInnerNavServiceProvider"
            ],
            "aliases": {
                "Rawilk": "Rawilk\\FilamentInnerNav\\Facades\\FilamentInnerNav"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
